{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings":{
        "react":{
            "version": "detect"
        }
    },
    "rules": {
        "react/prop-types":1,
        "indent": [
            1,
            4
        ],
        "camelcase": [
            0
        ],
        "valid-jsdoc": 1,
        "require-jsdoc": 0,
        "no-irregular-whitespace": 0,
        "no-invalid-this":0,
        "quotes":0,
        "padded-blocks":0,
        "key-spacing":0,
        "max-len":0,
        "comma-dangle":0,
        "no-trailing-spaces":0,
        "camelcase":0,
        "no-multiple-empty-lines":0,
        "guard-for-in":0,
        "keyword-spacing":0,
        "no-multi-spaces":0,
        "spaced-comment":0,
        "no-unused-vars":1,
        "new-cap":0
    }
}