{
  "name": "open-randomizer-tracker",
  "version": "0.1.0",
  "license": "BSD-4-Clause",
  "author": "Anthony Mattera",
  "private": true,
  "main": "build/electron.js",
  "dependencies": {
    "clone": "^2.1.2",
    "electron-store": "^3.2.0",
    "require": "^2.4.20"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "electron": "^4.0.6",
    "electron-builder": "^20.38.5",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.16.0",
    "file": "^0.2.2",
    "file-loader": "^4.2.0",
    "fs": "^0.0.1-security",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-rnd": "^10.0.0",
    "react-scripts": "2.1.5",
    "sass-loader": "^8.0.0",
    "system": "^2.0.1",
    "wait-on": "^3.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-glob-entry": "^2.1.1",
    "write-file-atomic": "^3.0.1"
  },
  "scripts": {
    "start": "electron ./build/electron.js & webpack --watch",
    "build": "rm -Rf ./build && rm -Rf ./dist && webpack && electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "build": {
    "appId": "open-randomizer-tracker",
    "artifactName": "open-randomizer-tracker-${version}.${ext}",
    "productName": "open-randomizer-tracker",
    "files": [
      "build/**/*"
    ],
    "linux": {
      "icon": "./build/resources/icons/icon.png",
      "target": "appimage",
      "category": "Multimedia"
    },
    "win": {
      "icon": "./build/resources/icons/icon.ico",
      "target": "dir"
    },
    "mac": {
      "icon": "./build/resources/icons/icon.icns",
      "target": "dmg"
    }
  }
}
